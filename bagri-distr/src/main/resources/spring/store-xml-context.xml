<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.2.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.2.xsd">

	<!-- set some common hive props in extra properties file -->
	<context:property-placeholder ignore-unresolvable="true" location="file:${xdm.config.path}/${xdm.config.properties.file}" />
    
	<bean id="docCacheStore" class="com.bagri.xdm.cache.hazelcast.store.xml.DocumentCacheStore">
	    <property name="dataPath" value="${xdm.schema.store.data.path}" />
	    <!--property name="documentManager" ref="docManager"/-->
	</bean>

	<bean id="xsdCacheStore" class="com.bagri.xdm.cache.hazelcast.store.xml.XsdCacheStore">
	    <!--property name="dataPath" value="${xdm.schema.store.schema.path}" /-->
		<!--property name="schemaDictionary" ref="xdmModel"/-->
	</bean>

</beans>