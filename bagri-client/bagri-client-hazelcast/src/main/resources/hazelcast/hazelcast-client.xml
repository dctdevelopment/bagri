<?xml version="1.0" encoding="UTF-8"?>
<hz:hazelcast-client xmlns:hz="http://www.hazelcast.com/schema/client-config" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.hazelcast.com/schema/client-config hazelcast-client-config-3.6.xsd">	
	
	<hz:group>
		<hz:name>default</hz:name>
		<hz:password>password</hz:password>
	</hz:group>

	<hz:network>
		<hz:cluster-members>
			<hz:address>localhost:10500</hz:address>
		</hz:cluster-members>
		<hz:smart-routing>true</hz:smart-routing>
		<hz:connection-timeout>20000</hz:connection-timeout>
		<hz:connection-attempt-limit>1</hz:connection-attempt-limit>
		<hz:socket-options>
			<hz:buffer-size>48</hz:buffer-size>
		</hz:socket-options>
	</hz:network>

	<hz:executor-pool-size>16</hz:executor-pool-size>

	<!--hz:security>
		<credentials>com.bagri.xdm.client.hazelcast.serialize.SecureCredentials</credentials>
	</hz:security-->

	<hz:serialization>
		<hz:data-serializable-factories>
			<hz:data-serializable-factory factory-id="1">com.bagri.xdm.client.hazelcast.serialize.DataSerializationFactoryImpl</hz:data-serializable-factory>
		</hz:data-serializable-factories>
		<hz:portable-factories>
			<hz:portable-factory factory-id="2">com.bagri.xdm.client.hazelcast.serialize.PortableFactoryImpl</hz:portable-factory>
		</hz:portable-factories>
		<hz:serializers>
			<!--hz:global-serializer class-name="com.hazelcast.nio.serialization.PortableSerializer"/-->
			<hz:serializer type-class="com.bagri.xdm.common.query.Expression" class-name="com.bagri.xdm.client.hazelcast.serialize.ExpressionSerializer"/>
			<hz:serializer type-class="com.bagri.xdm.common.query.AlwaysExpression" class-name="com.bagri.xdm.client.hazelcast.serialize.ExpressionSerializer"/>
			<hz:serializer type-class="com.bagri.xdm.common.query.PathExpression" class-name="com.bagri.xdm.client.hazelcast.serialize.ExpressionSerializer"/>
			<hz:serializer type-class="com.bagri.xdm.common.query.BinaryExpression" class-name="com.bagri.xdm.client.hazelcast.serialize.ExpressionSerializer"/>
			<hz:serializer type-class="com.bagri.xdm.common.query.ExpressionBuilder" class-name="com.bagri.xdm.client.hazelcast.serialize.ExpressionBuilderSerializer"/>
			<hz:serializer type-class="com.bagri.xdm.common.query.PathBuilder" class-name="com.bagri.xdm.client.hazelcast.serialize.PathBuilderSerializer"/>
			<hz:serializer type-class="com.bagri.xdm.common.query.ExpressionContainer" class-name="com.bagri.xdm.client.hazelcast.serialize.ExpressionContainerSerializer"/>
			<hz:serializer type-class="com.bagri.xdm.common.query.QueryBuilder" class-name="com.bagri.xdm.client.hazelcast.serialize.QueryBuilderSerializer"/>
			<hz:serializer type-class="com.bagri.xdm.common.query.QueriedPath" class-name="com.bagri.xdm.client.hazelcast.serialize.QueriedPathSerializer"/>
			<hz:serializer type-class="com.bagri.xdm.domain.Element" class-name="com.bagri.xdm.client.hazelcast.serialize.domain.ElementSerializer"/>
			<hz:serializer type-class="com.bagri.xdm.domain.Elements" class-name="com.bagri.xdm.client.hazelcast.serialize.domain.ElementsSerializer"/>
			<hz:serializer type-class="com.bagri.xdm.domain.Path" class-name="com.bagri.xdm.client.hazelcast.serialize.domain.PathSerializer"/>
			<hz:serializer type-class="com.bagri.xdm.domain.Document" class-name="com.bagri.xdm.client.hazelcast.serialize.domain.DocumentSerializer"/>
			<hz:serializer type-class="com.bagri.xdm.domain.DocumentType" class-name="com.bagri.xdm.client.hazelcast.serialize.domain.DocumentTypeSerializer"/>
			<hz:serializer type-class="com.bagri.xdm.domain.Namespace" class-name="com.bagri.xdm.client.hazelcast.serialize.domain.NamespaceSerializer"/>
			<hz:serializer type-class="com.bagri.xdm.domain.Query" class-name="com.bagri.xdm.client.hazelcast.serialize.domain.QuerySerializer"/>
			<hz:serializer type-class="com.bagri.xdm.domain.QueryResult" class-name="com.bagri.xdm.client.hazelcast.serialize.domain.QueryResultSerializer"/>
			<hz:serializer type-class="com.bagri.xdm.domain.IndexedDocument" class-name="com.bagri.xdm.client.hazelcast.serialize.domain.IndexedDocumentSerializer"/>
			<hz:serializer type-class="com.bagri.xdm.domain.UniqueDocument" class-name="com.bagri.xdm.client.hazelcast.serialize.domain.UniqueDocumentSerializer"/>
			<hz:serializer type-class="com.bagri.xdm.domain.UniqueValue" class-name="com.bagri.xdm.client.hazelcast.serialize.domain.UniqueValueSerializer"/>
			<hz:serializer type-class="com.bagri.xdm.domain.Transaction" class-name="com.bagri.xdm.client.hazelcast.serialize.domain.TransactionSerializer"/>
			<hz:serializer type-class="com.bagri.xdm.domain.FragmentedDocument" class-name="com.bagri.xdm.client.hazelcast.serialize.domain.FragmentedDocumentSerializer"/>
		</hz:serializers>
	</hz:serialization>

	<!--load-balancer type="random"></load-balancer-->
	 
	<hz:near-cache name="dict-path">
		<hz:in-memory-format>OBJECT</hz:in-memory-format>
	</hz:near-cache>
	<!--hz:near-cache name="dict-index">
		<hz:in-memory-format>OBJECT</hz:in-memory-format>
	</hz:near-cache-->
	<hz:near-cache name="dict-namespace">
		<hz:in-memory-format>OBJECT</hz:in-memory-format>
	</hz:near-cache>
	<hz:near-cache name="dict-document-type">
		<hz:in-memory-format>OBJECT</hz:in-memory-format>
	</hz:near-cache>
	
</hz:hazelcast-client>
